{
  "name": "@mapbox/mapbox-gl-geocoder",
  "version": "5.1.0",
  "description": "A geocoder control for Mapbox GL JS",
  "main": "lib/index.js",
  "unpkg": "dist/mapbox-gl-geocoder.min.js",
  "style": "lib/mapbox-gl-geocoder.css",
  "scripts": {
    "start": "vite --port 9967 --open debug/index.html",
    "prepublish": "NODE_ENV=production && mkdir -p dist && rollup -c && cp lib/mapbox-gl-geocoder.css dist/mapbox-gl-geocoder.css",
    "test": "vitest",
    "docs": "documentation build lib/index.js --format=md > API.md",
    "pretest": "npm run lint",
    "lint": "eslint lib test"
  },
  "files": [
    "lib",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mapbox/mapbox-gl-geocoder.git"
  },
  "keywords": [
    "geocoder",
    "osm",
    "gl"
  ],
  "author": "Mapbox",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mapbox/mapbox-gl-geocoder/issues"
  },
  "homepage": "https://github.com/mapbox/mapbox-gl-geocoder#readme",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitest/browser": "^3.2.4",
    "documentation": "^14.0.3",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^13.0.3",
    "lodash.once": "^4.0.0",
    "mapbox-gl": "^3.13.0",
    "minimist": "^1.2.6",
    "playwright": "^1.54.1",
    "rollup": "^4.45.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sinon": "^21.0.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.16.1",
    "events": "^3.3.0",
    "lodash.debounce": "^4.0.6",
    "nanoid": "^3.1.31",
    "subtag": "^0.5.0",
    "suggestions": "^1.6.0",
    "xtend": "^4.0.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
